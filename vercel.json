{
    "version": 2,
    "builds": [
        {
            "src": "/src/index.js",
            "use": "@vercal/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "installCommand": "pnpm install",
    "buildCommand": "nest build",
    "framework": "nodejs",
    "outputDirectory": "dist"
}